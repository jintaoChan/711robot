      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/link4.STL" scale="0.005 0.005 0.005"/>
        </geometry>
      </collision>
    </link>
    <joint name="${prefix}joint4" type="revolute">
      <parent link="${prefix}link3"/>
      <child link="${prefix}link4"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <axis xyz="0 0 -1"/>
      <dynamics damping="0.7"/>
      <limit effort="100" velocity="1.0"/>
    </joint>

    <!--  -->
    <link name="${prefix}link5">
      <visual>
      <origin
        xyz="-0.0043261798092078 0.0995606848099331 0.196143761355562"
        rpy="0 0 0" />
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/link5.STL" scale="0.005 0.005 0.005"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/link5.STL" scale="0.005 0.005 0.005"/>
        </geometry>
      </collision>
    </link>
    <joint name="${prefix}joint5" type="revolute">
      <parent link="${prefix}link4"/>
      <child link="${prefix}link5"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <axis xyz="0 0 -1"/>
      <dynamics damping="0.7"/>
      <limit effort="100" velocity="1.0"/>
    </joint>

    <!--  -->
    <link name="${prefix}link6">
      <visual>
      <origin
        xyz="0.000386879822536601 -0.0239742458499962 0.247370540938741"
        rpy="0 0 0" />
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/link6.STL" scale="0.005 0.005 0.005"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/link6.STL" scale="0.005 0.005 0.005"/>
