<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:macro name="TechRobot" params="parent prefix *origin">
    <!-- Constants for robot dimensions -->
    <xacro:property name="axel_offset" value="0.00" />

    <!-- Base Joint -->
    <joint name="${prefix}base_joint" type="fixed">
      <xacro:insert_block name="origin" />
      <parent link="${parent}"/>
      <child link="${prefix}base_link" />
    </joint>

    <link name="${prefix}base_link">
      <visual>
      <origin
        xyz="0.134546485528339 1.02602768939509 0.0276854546001787"
        rpy="0 0 0" />
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/base_link.STL" scale="0.005 0.005 0.005"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/base_link.STL" scale="0.005 0.005 0.005"/>
        </geometry>
      </collision>
    </link>

    <!--  -->
    <link name="${prefix}link0">
      <visual>
      <origin
        xyz="0.109505946566475 1.56830006166103 0.306532290779036"
        rpy="0 0 0" />
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/link0.STL" scale="0.005 0.005 0.005"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/link0.STL" scale="0.005 0.005 0.005"/>
        </geometry>
      </collision>
    </link>

    <joint name="${prefix}joint0" type="prismatic">
      <parent link="${prefix}base_link"/>
      <child link="${prefix}link0"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <axis xyz="0 -1 0"/>
      <dynamics damping="0.7"/>
      <limit effort="100" velocity="1.0"/>
    </joint>

    <!--  -->
    <link name="${prefix}link1">
      <visual>
      <origin
        xyz="0.0601483990121618 -0.443944828019226 1.73584633920143E-05"
        rpy="0 0 0" />
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/link1.STL" scale="0.005 0.005 0.005"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/link1.STL" scale="0.005 0.005 0.005"/>
        </geometry>
      </collision>
    </link>
    <joint name="${prefix}joint1" type="revolute">
      <parent link="${prefix}link0"/>
      <child link="${prefix}link1"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <axis xyz="0 1 0"/>
      <dynamics damping="0.7"/>
      <limit effort="100" velocity="1.0"/>
    </joint>

    <!--  -->
    <link name="${prefix}link2">
      <visual>
      <origin
        xyz="-0.16447187278791 0.0126535013198428 0.1148075556278"
        rpy="0 0 0" />
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/link2.STL" scale="0.005 0.005 0.005"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/link2.STL" scale="0.005 0.005 0.005"/>
        </geometry>
      </collision>
    </link>
    <joint name="${prefix}joint2" type="prismatic">
      <parent link="${prefix}link1"/>
      <child link="${prefix}link2"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <axis xyz="1 0 0"/>
      <dynamics damping="0.7"/>
      <limit effort="100" velocity="1.0"/>
    </joint>


    <!--  -->
    <link name="${prefix}link3">
      <visual>
      <origin
        xyz="-0.0679648513950829 -0.0125948841535013 0.0597190016437489"
        rpy="0 0 0" />
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/link3.STL" scale="0.005 0.005 0.005"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/link3.STL" scale="0.005 0.005 0.005"/>
        </geometry>
      </collision>
    </link>
    <joint name="${prefix}joint3" type="revolute">
      <parent link="${prefix}link2"/>
      <child link="${prefix}link3"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <axis xyz="1 0 0"/>
      <dynamics damping="0.7"/>
      <limit effort="100" velocity="1.0"/>
    </joint>

    <!--  -->
    <link name="${prefix}link4">
      <visual>
      <origin
        xyz="-2.79564493776974E-08 -0.00496995620296214 0.0493158936775911"
        rpy="0 0 0" />
        <geometry>
          <mesh filename="package://TechControlInterface/meshes/link4.STL" scale="0.005 0.005 0.005"/>
