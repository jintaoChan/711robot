      <origin xyz="0 0 0" rpy="0 0 0"/>
      <axis xyz="0 0 1"/>
      <dynamics damping="0.7"/>
      <limit effort="100" velocity="1.0"/>
    </joint>

  </xacro:macro>
</robot>